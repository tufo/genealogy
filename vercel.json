{
    "builds": [
        {
            "src": "app.py", 
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
        "src": "/(.*)", 
        "dest": "app.py"
        }
    ],
    "redirects": [
        { "source": "/", "destination": "/directory", "permanent": true }
      ]
}
